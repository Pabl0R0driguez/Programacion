package Parking;

public class VehiculosEntrada {

	private String matricula;
	private String tipo;
	private int plaza;
	
	public boolean entrada(int numvehiculo) {
		if(plaza>numvehiculo) {
			System.out.println("Puedes aparcar");
		}
		else {
			System.out.println("El parking est√° lleno vuelve en otro momento");
		}
		return false;
		
	}

	public char getMatricula() {
		return tipo;
	}
	
	public String getModelo() {
		return tipo;
	}
	
	public int getPlaza() {
		return plaza;
	}
	
	public void setMatricula(String matricula) {
		this.matricula = matricula;
	}

	public void setModelo(String tipo) {
		this.tipo = tipo;
	}

	public void setPlaza(int plaza) {
		this.plaza = plaza;
	}

	@Override
	public String toString() {
		return "VehiculosEntrada [matricula=" + matricula + ", modelo=" + tipo + ", plaza=" + plaza + "]";
	}
	

}


