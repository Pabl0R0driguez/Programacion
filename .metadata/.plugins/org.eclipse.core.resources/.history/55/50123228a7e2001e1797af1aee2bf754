package Parking;

import java.util.ArrayList;


public class Vehiculos {
	
	//Atributos
	private String matricula;
	private String tipo;
	private int plaza;
	
	//ArrayList
	private ArrayList<String> Coche;
	
	//Metodo entrada
	public boolean entrada(int numvehiculo) {
		if(plaza>numvehiculo) {
			Coche.add(numvehiculo, matricula);
		}
		else {
			System.out.println("El parking est√° lleno vuelve en otro momento");
		}
		return false;
		
	}

	//Getters
	public String getMatricula() {
		return tipo;
	}
	
	public String getModelo() {
		return tipo;
	}
	
	public int getPlaza() {
		return plaza;
	}
	
	//toString
	@Override
	public String toString() {
		return "VehiculosEntrada [matricula=" + matricula + ", modelo=" + tipo + ", plaza=" + plaza + "]";
	}
	

}


