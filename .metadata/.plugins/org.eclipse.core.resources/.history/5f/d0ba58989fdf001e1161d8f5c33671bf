package Prueba;

import java.util.ArrayList;

public class p1 {

	public static void main(String[] args) {
		
	ArrayList<Character> alfabeto = new ArrayList<>();
	//Alfabeto creado
	char [] lista = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','Ã±','o','o','p','q','r','r','s','t','u','w','x','y','w','x','y','z'};
		for(char numeros : lista) {
			alfabeto.add(numeros);
		}
	//Mostrar alfabeto
		System.out.println(alfabeto);
		
	//Letras E, H y O
		System.out.println("E" + alfabeto.indexOf('e'));
		System.out.println("H" + alfabeto.indexOf('h'));
		System.out.println("O" + alfabeto.indexOf('o'));
		
	//Eliminar elemento en la posicion 1 y u
		System.out.println("Posicion 1" + alfabeto.remove(1));
	}

}
